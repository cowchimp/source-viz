{
  "name": "source-viz-vscode",
  "displayName": "Hello VSCode Extension",
  "version": "0.1.0",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.49.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:source-viz-vscode.openSourceViz"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "source-viz-vscode.openSourceViz",
        "title": "Open Source Viz"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "source-viz-vscode-web": "0.1.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.49.0",
    "@types/glob": "^7.1.3",
    "@types/node": "^14.0.27",
    "glob": "^7.1.6",
    "typescript": "^3.9.7",
    "vscode-test": "^1.4.0"
  }
}
